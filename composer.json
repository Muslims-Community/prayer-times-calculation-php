{
    "name": "muslims-community/prayer-times-calculation",
    "description": "Minimalist & offline Prayer Times calculation package for PHP",
    "type": "library",
    "keywords": [
        "prayer-times",
        "islam",
        "salah",
        "offline",
        "calculation",
        "laravel",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mahmoud Alsamman",
            "email": "memoibraheem1@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "MuslimsCommunity\\PrayerTimes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MuslimsCommunity\\PrayerTimes\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuslimsCommunity\\PrayerTimes\\PrayerTimesServiceProvider"
            ],
            "aliases": {
                "PrayerTimes": "MuslimsCommunity\\PrayerTimes\\Facades\\PrayerTimes"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}